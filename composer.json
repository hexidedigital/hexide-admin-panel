{
    "name": "hexide-digital/hexide-admin-panel",
    "description": "hexide-digital admin panel",
    "license": "MIT",
    "type": "library",
    "version": "2.1.0",
    "authors": [
        {
            "name": "Oleksandr-Moik",
            "email": "sashamoik2016@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "HexideDigital\\HexideAdmin\\": "src/"
        },
        "files": [
            "./src/Functions/helpers.php"
        ]
    },
    "require": {
        "php": "^7.4|^8.0",
        "astrotomic/laravel-translatable": "^11.9",
        "hexide-digital/meta-tags-laravel": "^1.1",
        "hexide-digital/model-permissions": "^2.1",
        "jeroennoten/laravel-adminlte": "^3.6",
        "laravel/framework": "^8.0",
        "laravelcollective/html": "^6.2",
        "livewire/livewire": "^2.7",
        "rappasoft/laravel-livewire-tables": "^1.20",
        "intervention/image": "^2.7",
        "yoeunes/toastr": "^1.2"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "HexideDigital\\HexideAdmin\\Providers\\HexideAdminServiceProvider",
                "HexideDigital\\HexideAdmin\\Providers\\ThumbServiceProvider",
                "HexideDigital\\HexideAdmin\\Providers\\FileUploaderServiceProvider"
            ],
            "aliases": {
                "Thumb": "HexideDigital\\HexideAdmin\\Facades\\Thumb",
                "FileUploader": "HexideDigital\\HexideAdmin\\Facades\\FileUploader"
            }
        }
    }
}
